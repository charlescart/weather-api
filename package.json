    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "tsc": "tsc"
  },
  "keywords": [
    "weather",
    "api",
    "challenge",
    "make",
    "typescript",
    "jest",
    "nodejs",
    "javascript",
    "routing",
    "controller"
  ],
  "author": "Ing. Charles Rodriguez",
  "license": "ISC",
