    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "geoip-lite": "^1.4.2",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "multer": "^1.4.2",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "routing-controllers": "^0.9.0-alpha.4",
